  data: li_log_handle type bal_t_logh.

  insert g_log_handle into table li_log_handle.

  call function &apos;BAL_DB_SAVE&apos;
    exporting
      i_client       = sy-mandt
      i_save_all     = &apos;X&apos;
      i_t_log_handle = li_log_handle
    exceptions
      others         = 0.

endmethod.</source>
  </method>
